{
  "name": "@alyce/eloqua-spa",
  "version": "1.0.0",
  "license": "Alyce",
  "repository": "https://gitlab.alycedev.com/alycecom/eloqua-spa.git",
  "private": true,
  "scripts": {
    "types": "tsc",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --passWithNoTests",
    "test:update-snaps": "react-scripts test -u",
    "test:coverage": "react-scripts test --coverage --color",
    "prettier": "prettier --write ./src/**/*.js",
    "prettier:check": "prettier --check ./src/**/*.js",
    "lint:js": "eslint src --ext .js,.ts,.tsx,.jsx",
    "qa": "yarn lint:js && yarn prettier:check && yarn test --watchAll=false",
    "precommit": "yarn run types && lint-staged",
    "prepare": "[ \"$CI\" = \"true\" ] || husky install",
    "set-alycecom-version": "yarn add -E @alycecom/utils@$VERSION @alycecom/services@$VERSION @alycecom/ui@$VERSION @alycecom/modules@$VERSION @alycecom/hooks@$VERSION",
    "yalc:link": "yalc add @alycecom/modules @alycecom/services @alycecom/ui @alycecom/hooks @alycecom/utils"
  },
  "dependencies": {
    "@alycecom/hooks": "112.8.0",
    "@alycecom/modules": "112.8.0",
    "@alycecom/services": "112.8.0",
    "@alycecom/ui": "112.8.0",
    "@alycecom/utils": "112.8.0",
    "@emotion/react": "^11.7.1",
    "@emotion/styled": "^11.6.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.15",
    "@fortawesome/free-brands-svg-icons": "^5.7.2",
